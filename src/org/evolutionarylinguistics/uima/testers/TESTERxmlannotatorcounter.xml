<?xml version="1.0" encoding="UTF-8"?>
<analysisEngineDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <frameworkImplementation>org.apache.uima.java</frameworkImplementation>
  <primitive>false</primitive>
  <delegateAnalysisEngineSpecifiers>
    <delegateAnalysisEngine key="UIMAXmlAnnotator">
      <import name="org.evolutionarylinguistics.uima.xmlAnnotator.UIMAXmlAnnotator"/>
    </delegateAnalysisEngine>
    <delegateAnalysisEngine key="UIMAOccurenceCounter">
      <import name="org.evolutionarylinguistics.uima.occurenceCounter.UIMAOccurenceCounter"/>
    </delegateAnalysisEngine>
  </delegateAnalysisEngineSpecifiers>
  <analysisEngineMetaData>
    <name>TESTERxmlannotatorcounter</name>
    <description/>
    <version>1.0</version>
    <vendor/>
    <configurationParameters searchStrategy="language_fallback"/>
    <configurationParameterSettings/>
    <flowConstraints>
      <fixedFlow>
        <node>UIMAXmlAnnotator</node>
        <node>UIMAOccurenceCounter</node>
      </fixedFlow>
    </flowConstraints>
    <fsIndexCollection/>
    <capabilities>
      <capability>
        <inputs/>
        <outputs>
          <type allAnnotatorFeatures="true">org.evolutionarylinguistics.uima.xmlAnnotation</type>
          <type allAnnotatorFeatures="true">org.evolutionarylinguistics.uima.truthAnnotation</type>
        </outputs>
        <languagesSupported/>
      </capability>
    </capabilities>
    <operationalProperties>
      <modifiesCas>true</modifiesCas>
      <multipleDeploymentAllowed>true</multipleDeploymentAllowed>
      <outputsNewCASes>false</outputsNewCASes>
    </operationalProperties>
  </analysisEngineMetaData>
  <resourceManagerConfiguration/>
</analysisEngineDescription>
